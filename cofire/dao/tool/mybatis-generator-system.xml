<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动包位置 -->
    <classPathEntry location="D:\work\repository\mysql\mysql-connector-java\5.1.47\mysql-connector-java-5.1.47.jar" /> 
    <context id="DB2Tables" defaultModelType="flat" targetRuntime="MyBatis3">
    
    	<property name="javaFileEncoding" value="UTF-8" />
    
    	<plugin type="com.cofire.common.utils.mybatis.plugin.mutiDatasource.MutiDatasourcePaginationPlugin" />
    	<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
    	
        <commentGenerator type="com.cofire.common.utils.mybatis.plugin.RemarksCommentGenerator">
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!-- 数据库链接URL、用户名、密码 -->
         <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/cofire?characterEncoding=utf8" userId="root" password="root"> 
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.cofire.dao.model.system" targetProject="dao/src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="mapper.system" targetProject="dao/src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.cofire.dao.mapper.system" targetProject="dao/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
